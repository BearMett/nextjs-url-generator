{
  "name": "nextjs-url-generator",
  "displayName": "Next.js URL Generator",
  "publisher": "bearmett",
  "license": "MIT",
  "icon": "icon.png",
  "description": "Showing actual endpoint paths above HTTP method functions in Next.js API routes",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:javascript"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Next.js URL Generator",
      "properties": {
        "nextUrlGen.hostUrl": {
          "type": "string",
          "default": "http://localhost:3000",
          "description": "Set the host URL of the server"
        },
        "nextUrlGen.language": {
          "type": "string",
          "default": "en",
          "description": "Set the language for the extension",
          "enum": ["en", "ko"]
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "yarn run check-types && yarn run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "yarn run check-types && yarn run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "test:unit": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BearMett/nextjs-url-generator.git"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.6",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.24.0",
    "eslint": "^9.16.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.2",
    "vitest": "^3.0.4"
  },
  "dependencies": {
    "@babel/parser": "^7.26.7",
    "@babel/traverse": "^7.26.7"
  }
}
